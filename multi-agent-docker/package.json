{
  "name": "multi-agent-docker",
  "version": "1.0.0",
  "description": "A multi-agent Docker environment with Claude-Flow and other tools.",
  "private": true,
  "dependencies": {
    "claude-flow": "^2.0.0-alpha.110",
    "@google/gemini-cli": "latest",
    "openai": "latest",
    "@clduab11/gemini-flow": "latest",
    "gltf-pipeline": "^4.1.0",
    "sqlite3": "^5.1.7",
    "ws": "^8.18.0",
    "ruv-swarm": "latest"
  },
  "scripts": {
    "start-mcp-tcp": "node /app/core-assets/scripts/mcp-tcp-server.js",
    "start-mcp-ws": "node /app/core-assets/scripts/mcp-ws-relay.js",
    "start-gemini-flow": "gemini-flow serve --port 8080 --host 0.0.0.0",
    "start-claude-flow": "claude-flow serve --port 8081 --host 0.0.0.0",
    "start-playwright-proxy": "node /app/core-assets/scripts/playwright-mcp-proxy.js",
    "health-check": "node /app/core-assets/scripts/health-check.js",
    "init-agents": "bash /app/core-assets/scripts/init-claude-flow-agents.sh",
    "goal-init": "claude-flow goal init --force",
    "neural-init": "claude-flow neural init --force",
    "cf-status": "claude-flow status"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}