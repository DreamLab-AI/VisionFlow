@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix vocab: <http://example.org/vocab/> .

vocab:ontology a owl:Ontology ;
    rdfs:label "Test Ontology"@en ;
    owl:versionInfo "1.0" .

# Classes
foaf:Person a owl:Class ;
    rdfs:label "Person"@en .

vocab:Company a owl:Class ;
    rdfs:label "Company"@en .

vocab:File a owl:Class ;
    rdfs:label "File"@en .

# Disjoint classes
[ a owl:AllDisjointClasses ;
  owl:members ( foaf:Person vocab:Company vocab:File ) ] .

# Object properties
vocab:employedBy a owl:ObjectProperty ;
    rdfs:domain foaf:Person ;
    rdfs:range vocab:Company ;
    owl:inverseOf vocab:employs .

vocab:employs a owl:ObjectProperty ;
    rdfs:domain vocab:Company ;
    rdfs:range foaf:Person .

foaf:knows a owl:ObjectProperty, owl:SymmetricProperty ;
    rdfs:domain foaf:Person ;
    rdfs:range foaf:Person .

# Data properties
foaf:name a owl:DatatypeProperty ;
    rdfs:range xsd:string .

foaf:age a owl:DatatypeProperty ;
    rdfs:domain foaf:Person ;
    rdfs:range xsd:nonNegativeInteger .
