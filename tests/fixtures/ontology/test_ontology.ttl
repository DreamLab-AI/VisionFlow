@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix test: <http://example.org/test/> .

test:ontology a owl:Ontology ;
    rdfs:label "Test Ontology for Integration Tests"@en ;
    owl:versionInfo "1.0" ;
    rdfs:comment "Comprehensive test ontology covering common OWL features"@en .

# Classes
test:Person a owl:Class ;
    rdfs:label "Person"@en ;
    rdfs:comment "A human being"@en .

test:Employee a owl:Class ;
    rdfs:label "Employee"@en ;
    rdfs:comment "A person who is employed"@en ;
    rdfs:subClassOf test:Person .

test:Organization a owl:Class ;
    rdfs:label "Organization"@en ;
    rdfs:comment "A structured group of people"@en .

test:Company a owl:Class ;
    rdfs:label "Company"@en ;
    rdfs:comment "A commercial organization"@en ;
    rdfs:subClassOf test:Organization .

test:Document a owl:Class ;
    rdfs:label "Document"@en ;
    rdfs:comment "A written or electronic file"@en .

# Disjoint classes
[ a owl:AllDisjointClasses ;
  owl:members ( test:Person test:Organization test:Document ) ] .

# Object properties
test:worksFor a owl:ObjectProperty ;
    rdfs:label "works for"@en ;
    rdfs:domain test:Employee ;
    rdfs:range test:Organization ;
    owl:inverseOf test:employs .

test:employs a owl:ObjectProperty ;
    rdfs:label "employs"@en ;
    rdfs:domain test:Organization ;
    rdfs:range test:Employee .

test:knows a owl:ObjectProperty, owl:SymmetricProperty ;
    rdfs:label "knows"@en ;
    rdfs:domain test:Person ;
    rdfs:range test:Person ;
    rdfs:comment "Symmetric relationship between people"@en .

test:manages a owl:ObjectProperty ;
    rdfs:label "manages"@en ;
    rdfs:domain test:Employee ;
    rdfs:range test:Employee .

test:partOf a owl:ObjectProperty, owl:TransitiveProperty ;
    rdfs:label "part of"@en ;
    rdfs:comment "Transitive containment relationship"@en .

test:owns a owl:ObjectProperty ;
    rdfs:label "owns"@en ;
    rdfs:domain test:Person ;
    rdfs:range test:Document .

# Data properties
foaf:name a owl:DatatypeProperty ;
    rdfs:label "name"@en ;
    rdfs:range xsd:string .

foaf:age a owl:DatatypeProperty ;
    rdfs:label "age"@en ;
    rdfs:domain test:Person ;
    rdfs:range xsd:nonNegativeInteger .

test:hasSSN a owl:DatatypeProperty, owl:FunctionalProperty ;
    rdfs:label "has SSN"@en ;
    rdfs:domain test:Person ;
    rdfs:range xsd:string ;
    rdfs:comment "Functional property - each person has at most one SSN"@en .

foaf:email a owl:DatatypeProperty ;
    rdfs:label "email"@en ;
    rdfs:range xsd:string .

test:foundedYear a owl:DatatypeProperty ;
    rdfs:label "founded year"@en ;
    rdfs:domain test:Company ;
    rdfs:range xsd:gYear .

test:fileSize a owl:DatatypeProperty ;
    rdfs:label "file size"@en ;
    rdfs:domain test:Document ;
    rdfs:range xsd:nonNegativeInteger .

test:createdDate a owl:DatatypeProperty ;
    rdfs:label "created date"@en ;
    rdfs:range xsd:dateTime .

# Sample instances for testing
test:alice a test:Employee ;
    foaf:name "Alice Johnson" ;
    foaf:age 30 ;
    foaf:email "alice@example.com" ;
    test:hasSSN "123-45-6789" ;
    test:worksFor test:acmeCorp .

test:bob a test:Employee ;
    foaf:name "Bob Smith" ;
    foaf:age 35 ;
    foaf:email "bob@example.com" ;
    test:worksFor test:acmeCorp ;
    test:knows test:alice .

test:carol a test:Person ;
    foaf:name "Carol White" ;
    foaf:age 28 ;
    test:knows test:alice .

test:acmeCorp a test:Company ;
    foaf:name "ACME Corporation" ;
    test:foundedYear "1995"^^xsd:gYear ;
    test:employs test:alice ;
    test:employs test:bob .

test:report1 a test:Document ;
    foaf:name "Q4 Report" ;
    test:fileSize 1048576 ;
    test:createdDate "2025-01-15T10:30:00Z"^^xsd:dateTime .

# Additional relationships
test:alice test:manages test:bob .
test:alice test:owns test:report1 .
