  "test_scenarios": [
    {
      "name": "valid_employment_graph",
      "description": "Complete valid employment graph with all constraints satisfied",
      "nodes": [
        "john_smith", "jane_doe", "alice_chen",
        "acme_corp", "engineering_dept", "marketing_dept", "product_dept"
      ],
      "edges": [
        "emp_john_acme", "emp_jane_acme", "emp_alice_acme",
        "employs_john_acme", "employs_jane_acme", "employs_alice_acme",
        "dept_eng_acme", "dept_marketing_acme", "dept_product_acme",
        "works_john_eng", "works_jane_marketing", "works_alice_product"
      ],
      "expected_validation": "valid",
      "validation_checks": [
        "employment_consistency",
        "department_employment_consistency",
        "inverse_relationship_consistency",
        "domain_range_constraints"
      ]
    },
    {
      "name": "symmetric_colleague_relationships",
      "description": "Test symmetric colleague relationships and inference",
      "nodes": ["john_smith", "jane_doe", "alice_chen"],
      "edges": [
        "colleague_john_jane", "colleague_jane_john",
        "colleague_john_alice", "colleague_alice_john"
      ],
      "expected_validation": "valid",
      "validation_checks": [
        "colleague_symmetry_check",
        "no_self_colleague",
        "same_company_colleagues"
      ],
      "inferences": [
        "If A colleague_of B, then B colleague_of A"
      ]
    },
    {
      "name": "constraint_violation_examples",
      "description": "Examples of various constraint violations for testing",
      "test_cases": [
        {
          "name": "self_employment",
          "description": "Company employing itself",
          "modifications": {
            "add_edges": [
              {
                "from": "acme_corp",
                "to": "acme_corp",
                "type": "employs"
              }
            ]
          },
          "expected_validation": "invalid",
          "expected_errors": ["no_self_employment"]
        },
        {
          "name": "disjoint_class_violation",
          "description": "Node with multiple disjoint types",
          "modifications": {
            "modify_nodes": [
              {
                "id": "john_smith",
                "add_types": ["company"]
              }
            ]
          },
          "expected_validation": "invalid",
          "expected_errors": ["disjoint_class_violation"]
        },
        {
          "name": "invalid_age_range",
          "description": "Person with invalid age",
          "modifications": {
            "modify_nodes": [
              {
                "id": "john_smith",
                "properties": {
                  "test:hasAge": -10
                }
              }
            ]
          },
          "expected_validation": "invalid",
          "expected_errors": ["age_range_0_120"]
        },
        {
          "name": "missing_required_property",
          "description": "Person without required name",
          "modifications": {
            "modify_nodes": [
              {
                "id": "john_smith",
                "remove_properties": ["test:hasName"]
              }
            ]
          },
          "expected_validation": "invalid",
          "expected_errors": ["missing_required_property"]
        },
        {
          "name": "department_employment_inconsistency",
          "description": "Person works in dept of different company",
          "modifications": {
            "add_edges": [
              {
                "from": "john_smith",
                "to": "development_dept",
                "type": "works_in_department"
              }
            ]
          },
          "expected_validation": "invalid",
          "expected_errors": ["department_employment_consistency"]
        },
        {
          "name": "invalid_email_format",
          "description": "Person with malformed email address",
          "modifications": {
            "modify_nodes": [
              {
                "id": "jane_doe",
                "properties": {
                  "test:hasEmail": "invalid-email-format"
                }
              }
            ]
          },
          "expected_validation": "invalid",
          "expected_errors": ["email_format_validation"]
        },
        {
          "name": "duplicate_employee_id",
          "description": "Multiple persons with same employee ID",
          "modifications": {
            "modify_nodes": [
              {
                "id": "jane_doe",
                "properties": {
                  "test:hasEmployeeId": "EMP001"
                }
              }
            ]
          },
          "expected_validation": "invalid",
          "expected_errors": ["employee_id_unique"]
        }
      ]
    },
    {
      "name": "inference_testing",
      "description": "Test ontological inferences and derived relationships",
      "test_cases": [
        {
          "name": "inverse_property_inference",
          "description": "If A works_for B, then B employs A",
          "base_edges": [
            {"from": "john_smith", "to": "acme_corp", "type": "works_for"}
          ],
          "expected_inferences": [
            {"from": "acme_corp", "to": "john_smith", "type": "employs"}
          ]
        },
        {
          "name": "transitive_department_ownership",
          "description": "Department ownership through employment",
          "base_edges": [
            {"from": "john_smith", "to": "acme_corp", "type": "works_for"},
            {"from": "acme_corp", "to": "engineering_dept", "type": "has_department"},
            {"from": "john_smith", "to": "engineering_dept", "type": "works_in_department"}
          ],
          "expected_inferences": [
            "Person works for company that owns their department"
          ]
        }
      ]
    },
    {
      "name": "performance_testing_dataset",
      "description": "Large dataset for performance and scalability testing",
      "generation_rules": {
        "person_count": 100,
        "company_count": 10,
        "department_count": 30,
        "employment_ratio": 0.95,
        "colleague_density": 0.1,
        "department_assignment_ratio": 0.8
      },
      "validation_requirements": [
        "all_persons_have_names",
        "employment_consistency_maintained",
        "no_orphaned_departments",
        "colleague_symmetry_preserved"
      ]
    }
  ],

  "validation_configuration": {
    "strict_mode": true,
    "inference_enabled": true,
    "constraint_checking": "comprehensive",
    "error_reporting": "detailed",
    "performance_monitoring": true
  },

