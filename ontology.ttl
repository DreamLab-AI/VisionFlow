# Human Rights
metaverse:AI-0166 a owl:Class ;
    rdfs:label "Human Rights"@en ;
    rdfs:comment "Fundamental rights and freedoms inherent to all human beings as recognised in international instruments including the Universal Declaration of Human Rights, which AI systems must respect and protect throughout their lifecycle."@en ;
    skos:prefLabel "Human Rights"@en ;
    dcterms:identifier "AI-0166" ;
    dcterms:subject "metaverse" .

# Democratic Values
mv:DemocraticValues a owl:Class ;
    rdfs:label "Democratic Values"@en ;
    rdfs:comment "Principles and practices of democratic governance including pluralism, participatory decision-making, transparent governance, electoral integrity, free formation of political will and protection of civic space, which AI systems should respect and strengthen rather than undermine."@en ;
    skos:prefLabel "Democratic Values"@en ;
    dcterms:identifier "AI-0167" ;
    dcterms:subject "metaverse" ;
    dcterms:type "draft" ;
    owl:hasValue "ConceptualEntity" ;
    rdf:type owl:ConceptClass .

# Rule of Law
mv:RuleofLaw a owl:Class ;
    rdfs:label "Rule of Law"@en ;
    rdfs:comment "The principle that all persons, institutions and entities, including AI systems and their operators, are accountable to laws that are publicly promulgated, equally enforced, independently adjudicated and consistent with international human rights norms."@en ;
    skos:prefLabel "Rule of Law"@en ;
    dcterms:identifier "AI-0168" ;
    dcterms:subject "metaverse" ;
    dcterms:type "draft" ;
    owl:hasValue "ConceptualEntity" ;
    rdf:type owl:ConceptClass .

# Environmental Sustainability
mv:EnvironmentalSustainability a owl:Class ;
    rdfs:label "Environmental Sustainability"@en ;
    rdfs:comment "The responsible stewardship of natural resources and environmental systems in AI development and deployment, minimising ecological harm whilst potentially leveraging AI to address environmental challenges including climate change, biodiversity loss and resource depletion."@en ;
    skos:prefLabel "Environmental Sustainability"@en ;
    dcterms:identifier "AI-0169" ;
    dcterms:subject "metaverse" ;
    dcterms:type "draft" ;
    owl:hasValue "ConceptualEntity" ;
    rdf:type owl:ConceptClass .

# Social Impact
mv:SocialImpact a owl:Class ;
    rdfs:label "Social Impact"@en ;
    rdfs:comment "The effectspositive and negative, intended and unintendedthat AI systems have on individuals, communities, societies and social structures including employment, education, social cohesion, cultural practices and power distributions."@en ;
    skos:prefLabel "Social Impact"@en ;
    dcterms:identifier "AI-0170" ;
    dcterms:subject "metaverse" ;
    dcterms:type "draft" ;
    owl:hasValue "ConceptualEntity" ;
    rdf:type owl:ConceptClass .

# Transformer Architecture
mv:TransformerArchitecture a owl:Class ;
    rdfs:label "Transformer Architecture"@en ;
    rdfs:comment "A neural network architecture based solely on attention mechanisms, dispensing with recurrence and convolutions entirely, designed for sequence-to-sequence tasks."@en ;
    skos:prefLabel "Transformer Architecture"@en ;
    dcterms:identifier "AI-0196" ;
    dcterms:subject "metaverse" ;
    dcterms:type "draft" ;
    owl:hasValue "ConceptualEntity" ;
    rdf:type owl:ConceptClass .

# Self Attention
mv:SelfAttention a owl:Class ;
    rdfs:label "Self Attention"@en ;
    rdfs:comment "An attention mechanism where every token in a sequence attends to every other token in the same sequence, allowing the model to capture intra-sequence dependencies."@en ;
    skos:prefLabel "Self Attention"@en ;
    dcterms:identifier "AI-0197" ;
    dcterms:subject "metaverse" ;
    dcterms:type "draft" ;
    owl:hasValue "ConceptualEntity" ;
    rdf:type owl:ConceptClass .

# Multi Head Attention
mv:MultiHeadAttention a owl:Class ;
    rdfs:label "Multi Head Attention"@en ;
    rdfs:comment "An extension of the attention mechanism that allows the model to jointly attend to information from different representation subspaces at different positions, using multiple attention heads in parallel."@en ;
    skos:prefLabel "Multi Head Attention"@en ;
    dcterms:identifier "AI-0198" ;
    dcterms:subject "metaverse" ;
    dcterms:type "draft" ;
    owl:hasValue "ConceptualEntity" ;
    rdf:type owl:ConceptClass .

# Scaled Dot Product Attention
mv:ScaledDotProductAttention a owl:Class ;
    rdfs:label "Scaled Dot Product Attention"@en ;
    rdfs:comment "An attention mechanism that computes attention weights using the dot product of queries and keys, scaled by the square root of the key dimension, followed by a softmax normalisation."@en ;
    skos:prefLabel "Scaled Dot Product Attention"@en ;
    dcterms:identifier "AI-0199" ;
    dcterms:subject "metaverse" ;
    dcterms:type "draft" ;
    owl:hasValue "ConceptualEntity" ;
    rdf:type owl:ConceptClass .

# Query Key Value
mv:QueryKeyValue a owl:Class ;
    rdfs:label "Query Key Value"@en ;
    rdfs:comment "The three fundamental components in attention mechanisms: queries determine what information to seek, keys determine what information is available, and values contain the actual information to be retrieved."@en ;
    skos:prefLabel "Query Key Value"@en ;
    dcterms:identifier "AI-0200" ;
    dcterms:subject "metaverse" ;
    dcterms:type "draft" ;
    owl:hasValue "ConceptualEntity" ;
    rdf:type owl:ConceptClass .

# Positional Encoding
mv:PositionalEncoding a owl:Class ;
    rdfs:label "Positional Encoding"@en ;
    rdfs:comment "A technique for injecting information about the relative or absolute position of tokens in a sequence, essential for transformers since they lack inherent sequential ordering."@en ;
    skos:prefLabel "Positional Encoding"@en ;
    dcterms:identifier "AI-0201" ;
    dcterms:subject "metaverse" ;
    dcterms:type "draft" ;
    owl:hasValue "ConceptualEntity" ;
    rdf:type owl:ConceptClass .

# Feed Forward Network
mv:FeedForwardNetwork a owl:Class ;
    rdfs:label "Feed Forward Network"@en ;
    rdfs:comment "A fully connected neural network layer applied to each position separately and identically in a transformer, typically consisting of two linear transformations with a non-linear activation function."@en ;
    skos:prefLabel "Feed Forward Network"@en ;
    dcterms:identifier "AI-0202" ;
    dcterms:subject "metaverse" ;
    dcterms:type "draft" ;
    owl:hasValue "ConceptualEntity" ;
    rdf:type owl:ConceptClass .

# Layer Normalisation
mv:LayerNormalisation a owl:Class ;
    rdfs:label "Layer Normalisation"@en ;
    rdfs:comment "A normalisation technique that normalises activations across the feature dimension for each example independently, stabilising deep network training."@en ;
    skos:prefLabel "Layer Normalisation"@en ;
    dcterms:identifier "AI-0203" ;
    dcterms:subject "metaverse" ;
    dcterms:type "draft" ;
    owl:hasValue "ConceptualEntity" ;
    rdf:type owl:ConceptClass .

# Residual Connection
mv:ResidualConnection a owl:Class ;
    rdfs:label "Residual Connection"@en ;
    rdfs:comment "A neural network connection that adds the input of a layer to its output, enabling gradient flow in deep networks and facilitating training of very deep architectures."@en ;
    skos:prefLabel "Residual Connection"@en ;
    dcterms:identifier "AI-0204" ;
    dcterms:subject "metaverse" ;
    dcterms:type "draft" ;
    owl:hasValue "ConceptualEntity" ;
    rdf:type owl:ConceptClass .

# Encoder
mv:Encoder a owl:Class ;
    rdfs:label "Encoder"@en ;
    rdfs:comment "The component in an encoder-decoder architecture that processes the input sequence and produces contextualised representations, using self-attention and feed-forward layers."@en ;
    skos:prefLabel "Encoder"@en ;
    dcterms:identifier "AI-0205" ;
    dcterms:subject "metaverse" ;
    dcterms:type "draft" ;
    owl:hasValue "ConceptualEntity" ;
    rdf:type owl:ConceptClass .

# Decoder
mv:Decoder a owl:Class ;
    rdfs:label "Decoder"@en ;
    rdfs:comment "The component in an encoder-decoder architecture that generates the output sequence autoregressively, using masked self-attention, cross-attention to encoder outputs, and feed-forward layers."@en ;
    skos:prefLabel "Decoder"@en ;
    dcterms:identifier "AI-0206" ;
    dcterms:subject "metaverse" ;
    dcterms:type "draft" ;
    owl:hasValue "ConceptualEntity" ;
    rdf:type owl:ConceptClass .

# Encoder Decoder Architecture
mv:EncoderDecoderArchitecture a owl:Class ;
    rdfs:label "Encoder Decoder Architecture"@en ;
    rdfs:comment "A neural network structure consisting of an encoder that processes the input sequence and a decoder that generates the output sequence, commonly used in sequence-to-sequence tasks."@en ;
    skos:prefLabel "Encoder Decoder Architecture"@en ;
    dcterms:identifier "AI-0207" ;
    dcterms:subject "metaverse" ;
    dcterms:type "draft" ;
    owl:hasValue "ConceptualEntity" ;
    rdf:type owl:ConceptClass .

# Cross Attention
mv:CrossAttention a owl:Class ;
    rdfs:label "Cross Attention"@en ;
    rdfs:comment "An attention mechanism where queries come from one sequence whilst keys and values come from a different sequence, enabling information flow between the encoder and decoder in sequence-to-sequence models."@en ;
    skos:prefLabel "Cross Attention"@en ;
    dcterms:identifier "AI-0208" ;
    dcterms:subject "metaverse" ;
    dcterms:type "draft" ;
    owl:hasValue "ConceptualEntity" ;
    rdf:type owl:ConceptClass .

# Causal Attention
mv:CausalAttention a owl:Class ;
    rdfs:label "Causal Attention"@en ;
    rdfs:comment "An attention mechanism where each position can only attend to earlier positions in the sequence, preventing information flow from future tokens, essential for autoregressive generation."@en ;
    skos:prefLabel "Causal Attention"@en ;
    dcterms:identifier "AI-0209" ;
    dcterms:subject "metaverse" ;
    dcterms:type "draft" ;
    owl:hasValue "ConceptualEntity" ;
    rdf:type owl:ConceptClass .

# Attention Mask
mv:AttentionMask a owl:Class ;
    rdfs:label "Attention Mask"@en ;
    rdfs:comment "A mechanism that controls which positions in a sequence can attend to which other positions, typically implemented by adding large negative values before softmax to effectively zero out unwanted attention weights."@en ;
    skos:prefLabel "Attention Mask"@en ;
    dcterms:identifier "AI-0210" ;
    dcterms:subject "metaverse" ;
    dcterms:type "draft" ;
    owl:hasValue "ConceptualEntity" ;
    rdf:type owl:ConceptClass .

# BERT
mv:BERT a owl:Class ;
    rdfs:label "BERT"@en ;
    rdfs:comment "Bidirectional Encoder Representations from Transformers: a transformer-based model designed to pre-train deep bidirectional representations from unlabelled text by jointly conditioning on both left and right context in all layers."@en ;
    skos:prefLabel "BERT"@en ;
    dcterms:identifier "AI-0211" ;
    dcterms:subject "metaverse" ;
    dcterms:type "draft" ;
    owl:hasValue "ConceptualEntity" ;
    rdf:type owl:ConceptClass .

# GPT
mv:GPT a owl:Class ;
    rdfs:label "GPT"@en ;
    rdfs:comment "Generative Pre-trained Transformer: an autoregressive language model that uses transformer decoder architecture and is pre-trained on large text corpora using next-token prediction."@en ;
    skos:prefLabel "GPT"@en ;
    dcterms:identifier "AI-0212" ;
    dcterms:subject "metaverse" ;
    dcterms:type "draft" ;
    owl:hasValue "ConceptualEntity" ;
    rdf:type owl:ConceptClass .

# GPT 2
mv:GPT2 a owl:Class ;
    rdfs:label "GPT 2"@en ;
    rdfs:comment "A 1.5 billion parameter transformer language model trained simply to predict the next word in 40GB of internet text, demonstrating unsupervised multitask learning capabilities."@en ;
    skos:prefLabel "GPT 2"@en ;
    dcterms:identifier "AI-0213" ;
    dcterms:subject "metaverse" ;
    dcterms:type "draft" ;
    owl:hasValue "ConceptualEntity" ;
    rdf:type owl:ConceptClass .

# GPT 3
mv:GPT3 a owl:Class ;
    rdfs:label "GPT 3"@en ;
    rdfs:comment "An autoregressive language model with 175 billion parameters, 10 larger than any previous non-sparse language model, demonstrating few-shot and zero-shot learning capabilities without fine-tuning."@en ;
    skos:prefLabel "GPT 3"@en ;
    dcterms:identifier "AI-0214" ;
    dcterms:subject "metaverse" ;
    dcterms:type "draft" ;
    owl:hasValue "ConceptualEntity" ;
    rdf:type owl:ConceptClass .

# GPT 4
mv:GPT4 a owl:Class ;
    rdfs:label "GPT 4"@en ;
    rdfs:comment "A large-scale, multimodal transformer-based model that accepts image and text inputs and produces text outputs, exhibiting human-level performance on various professional and academic benchmarks."@en ;
    skos:prefLabel "GPT 4"@en ;
    dcterms:identifier "AI-0215" ;
    dcterms:subject "metaverse" ;
    dcterms:type "draft" ;
    owl:hasValue "ConceptualEntity" ;
    rdf:type owl:ConceptClass .

# T5
mv:T5 a owl:Class ;
    rdfs:label "T5"@en ;
    rdfs:comment "Text-to-Text Transfer Transformer: a unified framework that converts all NLP tasks into a text-to-text format, using an encoder-decoder architecture with a span-corruption pre-training objective."@en ;
    skos:prefLabel "T5"@en ;
    dcterms:identifier "AI-0216" ;
    dcterms:subject "metaverse" ;
    dcterms:type "draft" ;
    owl:hasValue "ConceptualEntity" ;
    rdf:type owl:ConceptClass .

# RoBERTa
mv:RoBERTa a owl:Class ;
    rdfs:label "RoBERTa"@en ;
