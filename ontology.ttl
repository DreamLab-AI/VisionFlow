    rdf:type owl:ConceptClass .

# Value Alignment
mv:ValueAlignment a owl:Class ;
    rdfs:label "Value Alignment"@en ;
    rdfs:comment "The challenge and process of ensuring AI systems pursue objectives that align with human values, even as those systems become more capable and autonomous. Value alignment addresses both technical and philosophical questions about encoding human preferences into AI behaviour."@en ;
    skos:prefLabel "Value Alignment"@en ;
    dcterms:identifier "AI-0269" ;
    dcterms:subject "metaverse" ;
    dcterms:type "draft" ;
