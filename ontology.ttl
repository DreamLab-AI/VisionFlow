] .

mv:Transparency(OECD) rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty ngm:enables ;
    owl:someValuesFrom metaverse:contestability
] .

mv:Transparency(OECD) rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty ngm:enables ;
    owl:someValuesFrom metaverse:trust
] .

# Explainability (OECD)
mv:Explainability(OECD) a owl:Class ;
    rdfs:label "Explainability (OECD)"@en ;
    rdfs:comment "People affected by AI-based outcomes should be able to understand how and why particular decisions or recommendations were reached, with explanations provided in ways appropriate to the context and enabling meaningful contestation of AI-influenced decisions."@en ;
    skos:prefLabel "Explainability (OECD)"@en ;
    dcterms:identifier "AI-0162" ;
    dcterms:subject "metaverse" ;
    dcterms:type "draft" ;
    owl:hasValue "ConceptualEntity" ;
    rdf:type owl:ConceptClass .

# Robustness (OECD)
mv:Robustness(OECD) a owl:Class ;
    rdfs:label "Robustness (OECD)"@en ;
    rdfs:comment "AI systems should function reliably and securely throughout their lifecycle, demonstrating resilience against errors, faults, inconsistencies and attempts to alter system use or performance, with continuous assessment and management of potential risks."@en ;
    skos:prefLabel "Robustness (OECD)"@en ;
    dcterms:identifier "AI-0163" ;
    dcterms:subject "metaverse" ;
    dcterms:type "draft" ;
    owl:hasValue "ConceptualEntity" ;
    rdf:type owl:ConceptClass .

# Safety (OECD)
mv:Safety(OECD) a owl:Class ;
    rdfs:label "Safety (OECD)"@en ;
    rdfs:comment "AI systems should operate safely without causing unacceptable risk of physical or psychological harm to people, property or the environment, with appropriate safeguards to prevent, detect and respond to hazardous failures throughout the AI lifecycle."@en ;
    skos:prefLabel "Safety (OECD)"@en ;
    dcterms:identifier "AI-0164" ;
    dcterms:subject "metaverse" ;
    dcterms:type "draft" ;
    owl:hasValue "ConceptualEntity" ;
    rdf:type owl:ConceptClass .

# Accountability (OECD)
mv:Accountability(OECD) a owl:Class ;
    rdfs:label "Accountability (OECD)"@en ;
    rdfs:comment "Organisations and individuals developing, deploying or operating AI systems should be accountable for their proper functioning in accordance with OECD AI Principles and applicable legal frameworks, based on their roles, context and ability to act."@en ;
    skos:prefLabel "Accountability (OECD)"@en ;
    dcterms:identifier "AI-0165" ;
    dcterms:subject "metaverse" ;
    dcterms:type "draft" ;
    owl:hasValue "ConceptualEntity" ;
