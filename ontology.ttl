# Blockchain Application
bc:BlockchainApplication a owl:Class ;
    rdfs:label "Blockchain Application"@en ;
    rdfs:comment "A decentralized application (dApp) that runs on a blockchain or peer-to-peer network rather than centralized servers, combining smart contract backend logic with frontend interfaces to provide enhanced security, transparency, censorship resistance, and zero downtime through distributed execution."@en ;
    skos:prefLabel "Blockchain Application"@en ;
    dcterms:identifier "BC-7010" ;
    dcterms:subject "bc" ;
    dcterms:type "production" ;
    owl:hasValue "ConceptualEntity" ;
    rdf:type owl:ConceptClass .

# Cryptographic Key Management
bc:CryptographicKeyManagement a owl:Class ;
    rdfs:label "Cryptographic Key Management"@en ;
    rdfs:comment "The administration of cryptographic keys throughout their lifecycle, including generation, storage, distribution, rotation, backup, recovery, and destruction, ensuring the security and availability of keying material while preventing unauthorized access or compromise. Governed by standards such as NIST SP 800-57."@en ;
    skos:prefLabel "Cryptographic Key Management"@en ;
    dcterms:identifier "BC-7011" ;
    dcterms:subject "bc" ;
    dcterms:type "reviewed" ;
    owl:hasValue "ConceptualEntity" ;
    rdf:type owl:ConceptClass .

# Smart Contracts
bc:SmartContracts a owl:Class ;
    rdfs:label "Smart Contracts"@en ;
    rdfs:comment "Self-executing programs stored on a blockchain that automatically enforce and execute the terms of an agreement when predetermined conditions are met, eliminating the need for intermediaries and enabling trustless, transparent, and immutable transaction automation. Smart contracts encode business logic in deterministic code that runs identically across all network nodes, ensuring consensus on execution outcomes."@en ;
    skos:prefLabel "Smart Contracts"@en ;
    dcterms:identifier "BC-7013" ;
    dcterms:subject "bc" ;
    dcterms:type "reviewed" ;
    owl:hasValue "ConceptualEntity" ;
    rdf:type owl:ProcessClass .

# Asymmetric Encryption
bc:AsymmetricEncryption a owl:Class ;
    rdfs:label "Asymmetric Encryption"@en ;
    rdfs:comment "A cryptographic system using mathematically related public-private key pairs where the public key encrypts data that only the corresponding private key can decrypt. Asymmetric encryption provides the foundational cryptographic primitive for blockchain identity, digital signatures, and secure communication, enabling trustless verification without pre-shared secrets."@en ;
    skos:prefLabel "Asymmetric Encryption"@en ;
    dcterms:identifier "BC-8001" ;
    dcterms:subject "bc" ;
    dcterms:type "reviewed" .

# Atomic Swap
bc:AtomicSwap a owl:Class ;
    rdfs:label "Atomic Swap"@en ;
    rdfs:comment "Trustless peer-to-peer cryptocurrency exchange mechanism across different blockchains using Hash Time-Locked Contracts (HTLCs) that guarantees atomic execution where the swap either completes fully or not at all, eliminating counterparty risk without requiring centralized intermediaries."@en ;
    skos:prefLabel "Atomic Swap"@en ;
    dcterms:identifier "BC-8002" ;
    dcterms:subject "bc" .

# Deterministic Finality
bc:DeterministicFinality a owl:Class ;
    rdfs:label "Deterministic Finality"@en ;
    rdfs:comment "Transaction finality achieved through explicit protocol mechanisms in BFT-based consensus systems, providing absolute mathematical guarantee that finalized blocks cannot be reverted. Requires supermajority validator agreement (typically 2/3+) and delivers instant certainty without probabilistic confirmation waiting."@en ;
    skos:prefLabel "Deterministic Finality"@en ;
    dcterms:identifier "BC-8004" ;
    dcterms:subject "bc" .

# Gas Optimization
bc:GasOptimization a owl:Class ;
    rdfs:label "Gas Optimization"@en ;
    rdfs:comment "Gas optimization is the systematic reduction of computational resources required for smart contract execution on blockchain networks, achieved through efficient storage patterns, opcode selection, data structure design, and batching strategies to minimise transaction costs and improve economic viability."@en ;
    skos:prefLabel "Gas Optimization"@en ;
    dcterms:identifier "BC-8006" ;
    dcterms:subject "bc" ;
    dcterms:type "reviewed" .

# Key Derivation Function
bc:KeyDerivationFunction a owl:Class ;
    rdfs:label "Key Derivation Function"@en ;
    rdfs:comment "A cryptographic algorithm that derives one or more secret keys from a master secret using a pseudo-random function, transforming human-readable passwords or seed phrases into cryptographically secure key material while enabling hierarchical key generation and deterministic wallet recovery in blockchain systems."@en ;
    skos:prefLabel "Key Derivation Function"@en ;
    dcterms:identifier "BC-8007" ;
    dcterms:subject "bc" ;
    dcterms:type "reviewed" .

# Layer 0
bc:Layer0 a owl:Class ;
    rdfs:label "Layer 0"@en ;
    rdfs:comment "Foundation blockchain infrastructure layer providing cross-chain communication protocols, shared security mechanisms, and modular consensus abstraction to enable interoperability between multiple Layer 1 blockchains. Layer 0 networks like Polkadot and Cosmos serve as the meta-layer coordinating heterogeneous blockchain ecosystems."@en ;
    skos:prefLabel "Layer 0"@en ;
    dcterms:identifier "BC-8008" ;
    dcterms:subject "bc" ;
    dcterms:type "reviewed" .

# Layer 3
bc:Layer3 a owl:Class ;
    rdfs:label "Layer 3"@en ;
    rdfs:comment "Application-specific blockchain layer built atop Layer 2 scaling solutions, providing customized execution environments for specialized use cases such as gaming, DeFi, and enterprise applications. Layer 3 inherits security from L2 while enabling hyperscalability through domain-specific transaction processing and optimized state management."@en ;
    skos:prefLabel "Layer 3"@en ;
    dcterms:identifier "BC-8009" ;
    dcterms:subject "bc" ;
    dcterms:type "reviewed" .

# Light Client
bc:LightClient a owl:Class ;
    rdfs:label "Light Client"@en ;
    rdfs:comment "Resource-efficient blockchain client that validates block headers and uses cryptographic proofs (Merkle proofs, state proofs) to verify transaction inclusion without downloading full blockchain state. Light clients enable trustless interaction from mobile devices, browsers, and IoT systems while requiring only megabytes of storage versus gigabytes for full nodes."@en ;
    skos:prefLabel "Light Client"@en ;
    dcterms:identifier "BC-8010" ;
    dcterms:subject "bc" ;
    dcterms:type "reviewed" .

# Lock and Mint Mechanism
bc:LockAndMintMechanism a owl:Class ;
    rdfs:label "Lock and Mint Mechanism"@en ;
    rdfs:comment "Cross-chain asset transfer protocol where tokens are locked in a smart contract on the source chain and equivalent wrapped tokens are minted on the destination chain, maintaining a 1:1 backing ratio. Enables asset portability across heterogeneous blockchain ecosystems while preserving value equivalence."@en ;
    skos:prefLabel "Lock and Mint Mechanism"@en ;
    dcterms:identifier "BC-8011" ;
    dcterms:subject "bc" .

# MEV
bc:Mev a owl:Class ;
    rdfs:label "MEV"@en ;
    rdfs:comment "Maximal Extractable Value (MEV) represents the profit that block producers can extract through strategic transaction ordering, inclusion, or exclusion within blocks, arising from their privileged position to control execution sequencing in blockchain networks."@en ;
    skos:prefLabel "MEV"@en ;
    dcterms:identifier "BC-8012" ;
    dcterms:subject "bc" ;
    dcterms:type "reviewed" .

# Probabilistic Finality
bc:ProbabilisticFinality a owl:Class ;
    rdfs:label "Probabilistic Finality"@en ;
    rdfs:comment "Probabilistic finality is a transaction confirmation model characteristic of longest-chain consensus mechanisms where confidence in transaction irreversibility increases exponentially with block depth, approaching but never reaching absolute certainty, with reversal probability decaying as (q/p)^k where q is attacker hashrate fraction and k is confirmation depth."@en ;
    skos:prefLabel "Probabilistic Finality"@en ;
    dcterms:identifier "BC-8013" ;
    dcterms:subject "bc" ;
    dcterms:type "reviewed" .

# Random Number Generation
bc:RandomNumberGeneration a owl:Class ;
    rdfs:label "Random Number Generation"@en ;
    rdfs:comment "The process of generating unpredictable and statistically random values for cryptographic operations, serving as a critical security primitive for key generation, nonces, and protocol initialization, distinguishing between true randomness from physical entropy sources and pseudo-randomness from deterministic algorithms seeded with entropy."@en ;
    skos:prefLabel "Random Number Generation"@en ;
    dcterms:identifier "BC-8014" ;
    dcterms:subject "bc" ;
    dcterms:type "reviewed" .

# Relayer
bc:Relayer a owl:Class ;
    rdfs:label "Relayer"@en ;
    rdfs:comment "Infrastructure operator that facilitates cross-chain message passing by monitoring source chains for events, generating cryptographic proofs of state, and submitting verified transactions to destination chains. Relayers provide non-custodial connectivity for interoperability protocols like IBC, LayerZero, and Chainlink CCIP without holding user assets."@en ;
    skos:prefLabel "Relayer"@en ;
    dcterms:identifier "BC-8015" ;
    dcterms:subject "bc" ;
    dcterms:type "reviewed" .

# Sidechain
bc:Sidechain a owl:Class ;
    rdfs:label "Sidechain"@en ;
    rdfs:comment "Independent blockchain connected to a parent blockchain via a two-way peg mechanism that operates with its own consensus and validation rules while enabling asset transfers between chains, providing scalability and experimental capabilities without impacting the main chain."@en ;
    skos:prefLabel "Sidechain"@en ;
    dcterms:identifier "BC-8016" ;
    dcterms:subject "bc" .

# State Channel
bc:StateChannel a owl:Class ;
    rdfs:label "State Channel"@en ;
    rdfs:comment "State channels are Layer 2 scaling solutions enabling off-chain interactions between participants through signed state updates, requiring only on-chain transactions for channel opening, closing, and dispute resolution, thereby achieving instant finality and near-zero marginal transaction costs."@en ;
    skos:prefLabel "State Channel"@en ;
    dcterms:identifier "BC-8017" ;
    dcterms:subject "bc" ;
    dcterms:type "reviewed" .

# Symmetric Encryption
bc:SymmetricEncryption a owl:Class ;
    rdfs:label "Symmetric Encryption"@en ;
    rdfs:comment "A cryptographic method using a single shared secret key for both encryption and decryption, requiring secure key exchange between parties before communication. Symmetric encryption provides confidentiality for data at rest and in transit with high computational efficiency, making it suitable for bulk data encryption in blockchain and general security applications."@en ;
    skos:prefLabel "Symmetric Encryption"@en ;
    dcterms:identifier "BC-8018" ;
    dcterms:subject "bc" ;
    dcterms:type "reviewed" .

# Synchronous Execution
bc:SynchronousExecution a owl:Class ;
    rdfs:label "Synchronous Execution"@en ;
    rdfs:comment "Synchronous Execution is a computational model where operations are performed sequentially, with each operation blocking until completion before the next operation begins. The caller waits for the result before proceeding, establishing a direct temporal dependency between operations and enabling predictable, deterministic execution flow."@en ;
    skos:prefLabel "Synchronous Execution"@en ;
    dcterms:identifier "BC-8019" ;
    dcterms:subject "bc" ;
    dcterms:type "reviewed" .

# Transaction Finality
bc:TransactionFinality a owl:Class ;
    rdfs:label "Transaction Finality"@en ;
    rdfs:comment "The point at which a blockchain transaction becomes irreversible and cannot be altered or removed, providing settlement certainty for participants. Finality types include probabilistic (increasing confidence with confirmations), deterministic (explicit protocol guarantee), and economic (cost to revert exceeds benefit)."@en ;
    skos:prefLabel "Transaction Finality"@en ;
    dcterms:identifier "BC-8021" ;
    dcterms:subject "bc" .

# Cosmos IBC
bc:CosmosIBC a owl:Class ;
    rdfs:label "Cosmos IBC"@en ;
    rdfs:comment "Inter-Blockchain Communication protocol that enables different blockchain networks to exchange data and tokens, providing trustless interoperability in the Cosmos ecosystem."@en ;
    skos:prefLabel "Cosmos IBC"@en ;
    dcterms:identifier "BC-9001" ;
    dcterms:subject "bc" ;
    dcterms:type "emerging" ;
    owl:hasValue "Abstract" ;
    rdf:type owl:ProcessClass .

# Polkadot Parachains
bc:PolkadotParachains a owl:Class ;
    rdfs:label "Polkadot Parachains"@en ;
    rdfs:comment "Application-specific blockchains that connect to and are secured by the Polkadot Relay Chain, enabling interoperability and shared security across heterogeneous blockchain networks."@en ;
    skos:prefLabel "Polkadot Parachains"@en ;
    dcterms:identifier "BC-9002" ;
    dcterms:subject "bc" ;
    dcterms:type "emerging" ;
    owl:hasValue "Abstract" ;
    rdf:type owl:ProcessClass .

# Cross-Chain Messaging
bc:CrossChainMessaging a owl:Class ;
    rdfs:label "Cross-Chain Messaging"@en ;
    rdfs:comment "Protocols and mechanisms that enable communication and data transfer between different blockchain networks, facilitating interoperability and cross-chain applications."@en ;
    skos:prefLabel "Cross-Chain Messaging"@en ;
    dcterms:identifier "BC-9003" ;
    dcterms:subject "bc" ;
    dcterms:type "emerging" ;
    owl:hasValue "Abstract" ;
    rdf:type owl:ProcessClass .

# Account Abstraction
bc:AccountAbstraction a owl:Class ;
    rdfs:label "Account Abstraction"@en ;
    rdfs:comment "ERC-4337 standard that enables smart contract wallets with programmable logic, allowing users to customize transaction validation, gas payment, and account recovery without protocol changes."@en ;
    skos:prefLabel "Account Abstraction"@en ;
    dcterms:identifier "BC-9005" ;
    dcterms:subject "bc" ;
    dcterms:type "emerging" ;
    owl:hasValue "Abstract" ;
    rdf:type owl:ProcessClass .

