# AI Grounded Ontology - Refactored Edition
# Transformation from SHACL-only to Rich OWL Class Hierarchy
# Version: 2.0.0
# Generated: 2025-10-28
# Compliance: Metaverse Ontology Quality Standards
# Total Classes: 72 explicit OWL classes
# Hierarchy Depth: 3-5 parents per class average

@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix aigo: <http://purl.org/ai-grounded-ontology/core#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dcterms: <http://purl.org/dc/terms/> .

# ============================================================================
# ONTOLOGY DECLARATION
# ============================================================================

<http://purl.org/ai-grounded-ontology/core> rdf:type owl:Ontology ;
    rdfs:label "AI Grounded Ontology"@en ;
    rdfs:comment "Comprehensive ontology for artificial intelligence systems, models, risks, training, and integration patterns"@en ;
    owl:versionInfo "2.0.0" ;
    dcterms:created "2025-10-28"^^xsd:date ;
    dcterms:description "Rich OWL class hierarchy for AI/ML domain with 72 classes across 5 major categories"@en .

# ============================================================================
# CATEGORY 1: AI MODEL TAXONOMY (20 classes)
# Machine learning approaches, generative/discriminative models, foundation models
# ============================================================================

# === Core Machine Learning Approaches ===

aigo:MachineLearningApproach rdf:type owl:Class ;
    rdfs:label "Machine Learning Approach"@en ;
    rdfs:comment "Fundamental paradigm for training computational models from data to make predictions or decisions without explicit programming"@en .

aigo:SupervisedLearning rdf:type owl:Class ;
    rdfs:subClassOf aigo:MachineLearningApproach ;
    rdfs:subClassOf aigo:LabeledDataRequired ;
    rdfs:subClassOf aigo:PredictiveModel ;
    rdfs:label "Supervised Learning"@en ;
    rdfs:comment "Machine learning approach using labeled training data where models learn mapping from inputs to known outputs"@en .

aigo:UnsupervisedLearning rdf:type owl:Class ;
    rdfs:subClassOf aigo:MachineLearningApproach ;
    rdfs:subClassOf aigo:UnlabeledDataProcessing ;
    rdfs:subClassOf aigo:PatternDiscovery ;
    rdfs:label "Unsupervised Learning"@en ;
    rdfs:comment "Machine learning approach discovering patterns and structures in unlabeled data without predefined categories"@en .

aigo:ReinforcementLearning rdf:type owl:Class ;
    rdfs:subClassOf aigo:MachineLearningApproach ;
    rdfs:subClassOf aigo:SequentialDecisionMaking ;
    rdfs:subClassOf aigo:RewardBasedOptimization ;
    rdfs:label "Reinforcement Learning"@en ;
    rdfs:comment "Machine learning approach where agents learn optimal behaviors through trial-and-error interaction with environment using reward signals"@en .

aigo:SemiSupervisedLearning rdf:type owl:Class ;
    rdfs:subClassOf aigo:MachineLearningApproach ;
    rdfs:subClassOf aigo:HybridLearningStrategy ;
    rdfs:subClassOf aigo:LabeledDataRequired ;
    rdfs:subClassOf aigo:UnlabeledDataProcessing ;
    rdfs:label "Semi-Supervised Learning"@en ;
    rdfs:comment "Machine learning approach combining small amounts of labeled data with large amounts of unlabeled data during training"@en .

aigo:SelfSupervisedLearning rdf:type owl:Class ;
    rdfs:subClassOf aigo:MachineLearningApproach ;
    rdfs:subClassOf aigo:UnlabeledDataProcessing ;
    rdfs:subClassOf aigo:AutomaticLabelGeneration ;
    rdfs:label "Self-Supervised Learning"@en ;
    rdfs:comment "Machine learning approach that generates supervisory signals from the data itself without human annotation"@en .

# === Generative Models ===

aigo:GenerativeModel rdf:type owl:Class ;
    rdfs:subClassOf aigo:MachineLearningModel ;
    rdfs:subClassOf aigo:DataSynthesis ;
    rdfs:subClassOf aigo:ProbabilisticModeling ;
    rdfs:label "Generative Model"@en ;
    rdfs:comment "Model that learns to generate new data instances by modeling the underlying data distribution"@en .

aigo:GenerativeAdversarialNetwork rdf:type owl:Class ;
    rdfs:subClassOf aigo:GenerativeModel ;
    rdfs:subClassOf aigo:AdversarialTraining ;
    rdfs:subClassOf aigo:DualNetworkArchitecture ;
    rdfs:label "Generative Adversarial Network"@en ;
    rdfs:comment "Generative model using two competing neural networks (generator and discriminator) trained adversarially to produce realistic synthetic data"@en .

aigo:VariationalAutoencoder rdf:type owl:Class ;
    rdfs:subClassOf aigo:GenerativeModel ;
    rdfs:subClassOf aigo:LatentSpaceModeling ;
    rdfs:subClassOf aigo:ProbabilisticEncoding ;
    rdfs:label "Variational Autoencoder"@en ;
    rdfs:comment "Generative model that learns compressed latent representations and probabilistic decoding for data generation"@en .

aigo:DiffusionModel rdf:type owl:Class ;
    rdfs:subClassOf aigo:GenerativeModel ;
    rdfs:subClassOf aigo:IterativeDenoising ;
    rdfs:subClassOf aigo:StochasticProcess ;
    rdfs:label "Diffusion Model"@en ;
    rdfs:comment "Generative model that learns to reverse a gradual noising process to generate high-quality samples"@en .

aigo:AutoregressiveModel rdf:type owl:Class ;
    rdfs:subClassOf aigo:GenerativeModel ;
    rdfs:subClassOf aigo:SequentialGeneration ;
    rdfs:subClassOf aigo:ConditionalProbability ;
    rdfs:label "Autoregressive Model"@en ;
    rdfs:comment "Generative model that produces data sequentially, conditioning each element on previously generated elements"@en .

# === Discriminative Models ===

aigo:DiscriminativeModel rdf:type owl:Class ;
    rdfs:subClassOf aigo:MachineLearningModel ;
    rdfs:subClassOf aigo:DecisionBoundaryLearning ;
    rdfs:subClassOf aigo:ConditionalModeling ;
    rdfs:label "Discriminative Model"@en ;
    rdfs:comment "Model that learns decision boundaries to classify or predict outputs given inputs"@en .

aigo:ClassificationModel rdf:type owl:Class ;
    rdfs:subClassOf aigo:DiscriminativeModel ;
    rdfs:subClassOf aigo:SupervisedLearning ;
    rdfs:subClassOf aigo:CategoryPrediction ;
    rdfs:label "Classification Model"@en ;
    rdfs:comment "Discriminative model that assigns input data to predefined categorical classes"@en .

aigo:RegressionModel rdf:type owl:Class ;
    rdfs:subClassOf aigo:DiscriminativeModel ;
    rdfs:subClassOf aigo:SupervisedLearning ;
    rdfs:subClassOf aigo:ContinuousValuePrediction ;
    rdfs:label "Regression Model"@en ;
    rdfs:comment "Discriminative model that predicts continuous numerical values from input features"@en .

aigo:ClusteringModel rdf:type owl:Class ;
    rdfs:subClassOf aigo:UnsupervisedLearning ;
    rdfs:subClassOf aigo:PatternDiscovery ;
    rdfs:subClassOf aigo:GroupingAlgorithm ;
    rdfs:label "Clustering Model"@en ;
    rdfs:comment "Unsupervised model that groups similar data points into clusters based on feature similarity"@en .

# === Foundation and Large Models ===

aigo:FoundationModel rdf:type owl:Class ;
    rdfs:subClassOf aigo:MachineLearningModel ;
    rdfs:subClassOf aigo:LargeScalePretraining ;
    rdfs:subClassOf aigo:TransferLearningCapable ;
    rdfs:subClassOf aigo:MultiTaskAdaptable ;
    rdfs:label "Foundation Model"@en ;
    rdfs:comment "Large-scale model trained on broad data that can be adapted to a wide range of downstream tasks"@en .

aigo:LargeLanguageModel rdf:type owl:Class ;
    rdfs:subClassOf aigo:FoundationModel ;
    rdfs:subClassOf aigo:TextProcessing ;
    rdfs:subClassOf aigo:NaturalLanguageUnderstanding ;
    rdfs:subClassOf aigo:ContextualRepresentation ;
    rdfs:label "Large Language Model"@en ;
    rdfs:comment "Foundation model trained on vast text corpora for natural language understanding and generation tasks"@en .

aigo:VisionModel rdf:type owl:Class ;
    rdfs:subClassOf aigo:FoundationModel ;
    rdfs:subClassOf aigo:ImageProcessing ;
    rdfs:subClassOf aigo:VisualPatternRecognition ;
    rdfs:label "Vision Model"@en ;
    rdfs:comment "Foundation model specialized for processing and understanding visual information from images or videos"@en .

aigo:MultimodalModel rdf:type owl:Class ;
    rdfs:subClassOf aigo:FoundationModel ;
    rdfs:subClassOf aigo:CrossModalityLearning ;
    rdfs:subClassOf aigo:IntegratedRepresentation ;
    rdfs:subClassOf aigo:MultiSensoryProcessing ;
    rdfs:label "Multimodal Model"@en ;
    rdfs:comment "Foundation model that processes and integrates information from multiple modalities such as text, image, audio, and video"@en .

# === Hybrid and Ensemble Approaches ===

aigo:EnsembleModel rdf:type owl:Class ;
    rdfs:subClassOf aigo:MachineLearningModel ;
    rdfs:subClassOf aigo:MultiModelAggregation ;
    rdfs:subClassOf aigo:PredictionCombination ;
    rdfs:label "Ensemble Model"@en ;
    rdfs:comment "Model that combines predictions from multiple base models to improve overall performance and robustness"@en .

# ============================================================================
# CATEGORY 2: NEURAL NETWORK ARCHITECTURES (15 classes)
# CNN, RNN, GNN, Transformer variants, attention mechanisms
# ============================================================================

aigo:NeuralNetworkArchitecture rdf:type owl:Class ;
    rdfs:label "Neural Network Architecture"@en ;
    rdfs:comment "Structural design pattern of artificial neural network defining layers, connections, and information flow"@en .

# === Convolutional Neural Networks ===

aigo:ConvolutionalNeuralNetwork rdf:type owl:Class ;
    rdfs:subClassOf aigo:NeuralNetworkArchitecture ;
    rdfs:subClassOf aigo:ImageProcessing ;
    rdfs:subClassOf aigo:SpatialFeatureExtraction ;
    rdfs:label "Convolutional Neural Network"@en ;
    rdfs:comment "Neural network architecture using convolutional layers to automatically learn spatial hierarchies of features from images"@en .

aigo:ResidualNetwork rdf:type owl:Class ;
    rdfs:subClassOf aigo:ConvolutionalNeuralNetwork ;
    rdfs:subClassOf aigo:SkipConnectionArchitecture ;
    rdfs:subClassOf aigo:DeepNetworkOptimization ;
    rdfs:label "Residual Network"@en ;
    rdfs:comment "Deep convolutional network using skip connections to enable training of very deep architectures by mitigating vanishing gradients"@en .

aigo:EfficientNet rdf:type owl:Class ;
    rdfs:subClassOf aigo:ConvolutionalNeuralNetwork ;
    rdfs:subClassOf aigo:ScalableArchitecture ;
    rdfs:subClassOf aigo:CompoundScaling ;
    rdfs:label "EfficientNet"@en ;
    rdfs:comment "Convolutional network family optimized for efficiency by systematically scaling network depth, width, and resolution"@en .

# === Recurrent Neural Networks ===

aigo:RecurrentNeuralNetwork rdf:type owl:Class ;
    rdfs:subClassOf aigo:NeuralNetworkArchitecture ;
    rdfs:subClassOf aigo:SequentialDataProcessing ;
    rdfs:subClassOf aigo:TemporalDependencyModeling ;
    rdfs:label "Recurrent Neural Network"@en ;
    rdfs:comment "Neural network architecture with recurrent connections allowing information persistence across sequential inputs"@en .

aigo:LongShortTermMemory rdf:type owl:Class ;
    rdfs:subClassOf aigo:RecurrentNeuralNetwork ;
    rdfs:subClassOf aigo:GatedMemoryCell ;
    rdfs:subClassOf aigo:LongRangeDependency ;
    rdfs:label "Long Short-Term Memory"@en ;
    rdfs:comment "Recurrent network variant with gated memory cells designed to capture long-range temporal dependencies"@en .

aigo:GatedRecurrentUnit rdf:type owl:Class ;
    rdfs:subClassOf aigo:RecurrentNeuralNetwork ;
    rdfs:subClassOf aigo:GatedMemoryCell ;
    rdfs:subClassOf aigo:SimplifiedArchitecture ;
    rdfs:label "Gated Recurrent Unit"@en ;
    rdfs:comment "Simplified recurrent network variant with gating mechanisms for capturing temporal dependencies with fewer parameters than LSTM"@en .

aigo:BidirectionalRNN rdf:type owl:Class ;
    rdfs:subClassOf aigo:RecurrentNeuralNetwork ;
    rdfs:subClassOf aigo:DualDirectionProcessing ;
    rdfs:subClassOf aigo:ContextualEncoding ;
    rdfs:label "Bidirectional Recurrent Neural Network"@en ;
    rdfs:comment "Recurrent network processing sequences in both forward and backward directions for enhanced contextual understanding"@en .

# === Graph Neural Networks ===

aigo:GraphNeuralNetwork rdf:type owl:Class ;
    rdfs:subClassOf aigo:NeuralNetworkArchitecture ;
    rdfs:subClassOf aigo:GraphStructureProcessing ;
    rdfs:subClassOf aigo:NodeRepresentationLearning ;
    rdfs:label "Graph Neural Network"@en ;
    rdfs:comment "Neural network architecture operating on graph-structured data to learn node and edge representations"@en .

aigo:GraphConvolutionalNetwork rdf:type owl:Class ;
    rdfs:subClassOf aigo:GraphNeuralNetwork ;
    rdfs:subClassOf aigo:SpectralGraphProcessing ;
    rdfs:subClassOf aigo:NeighborhoodAggregation ;
    rdfs:label "Graph Convolutional Network"@en ;
    rdfs:comment "Graph neural network using convolutional operations that aggregate information from node neighborhoods"@en .

aigo:GraphAttentionNetwork rdf:type owl:Class ;
    rdfs:subClassOf aigo:GraphNeuralNetwork ;
    rdfs:subClassOf aigo:AttentionMechanism ;
    rdfs:subClassOf aigo:WeightedNeighborAggregation ;
    rdfs:label "Graph Attention Network"@en ;
    rdfs:comment "Graph neural network using attention mechanisms to learn importance weights for neighboring nodes"@en .

# === Transformer Architectures ===

aigo:TransformerArchitecture rdf:type owl:Class ;
    rdfs:subClassOf aigo:NeuralNetworkArchitecture ;
    rdfs:subClassOf aigo:AttentionMechanism ;
    rdfs:subClassOf aigo:ParallelProcessing ;
    rdfs:subClassOf aigo:PositionalEncoding ;
    rdfs:label "Transformer Architecture"@en ;
    rdfs:comment "Neural network architecture based on self-attention mechanisms for processing sequential data in parallel"@en .

aigo:VisionTransformer rdf:type owl:Class ;
    rdfs:subClassOf aigo:TransformerArchitecture ;
    rdfs:subClassOf aigo:ImageProcessing ;
    rdfs:subClassOf aigo:PatchBasedProcessing ;
    rdfs:label "Vision Transformer"@en ;
    rdfs:comment "Transformer architecture adapted for computer vision by processing images as sequences of patches"@en .

aigo:MultimodalTransformer rdf:type owl:Class ;
    rdfs:subClassOf aigo:TransformerArchitecture ;
    rdfs:subClassOf aigo:CrossModalityLearning ;
    rdfs:subClassOf aigo:JointRepresentation ;
    rdfs:label "Multimodal Transformer"@en ;
    rdfs:comment "Transformer architecture designed to process and align information from multiple modalities simultaneously"@en .

# === Attention Mechanisms ===

aigo:AttentionMechanism rdf:type owl:Class ;
    rdfs:subClassOf aigo:NeuralNetworkArchitecture ;
    rdfs:subClassOf aigo:SelectiveFocusProcessing ;
    rdfs:subClassOf aigo:WeightedRepresentation ;
    rdfs:label "Attention Mechanism"@en ;
    rdfs:comment "Neural network component that learns to selectively focus on relevant parts of input data"@en .

aigo:SelfAttention rdf:type owl:Class ;
    rdfs:subClassOf aigo:AttentionMechanism ;
    rdfs:subClassOf aigo:IntraSequenceAttention ;
    rdfs:subClassOf aigo:ContextualRepresentation ;
    rdfs:label "Self-Attention"@en ;
    rdfs:comment "Attention mechanism relating different positions within a single sequence to compute contextual representations"@en .

# ============================================================================
# CATEGORY 3: AI RISK & GOVERNANCE (20 classes)
# Bias/fairness, safety/robustness, governance frameworks, explainability
# ============================================================================

# === Core Risk Concepts ===

aigo:AIRisk rdf:type owl:Class ;
    rdfs:label "AI Risk"@en ;
    rdfs:comment "Potential negative impact or harm associated with development, deployment, or operation of AI systems"@en .

aigo:BiasAndFairness rdf:type owl:Class ;
    rdfs:subClassOf aigo:AIRisk ;
    rdfs:subClassOf aigo:EthicalConcern ;
    rdfs:subClassOf aigo:DiscriminationPrevention ;
    rdfs:label "Bias and Fairness"@en ;
    rdfs:comment "Risk category addressing systematic errors and unfair treatment in AI decision-making across different demographic groups"@en .

# === Bias Types ===

aigo:AlgorithmicBias rdf:type owl:Class ;
    rdfs:subClassOf aigo:BiasAndFairness ;
    rdfs:subClassOf aigo:SystematicError ;
    rdfs:subClassOf aigo:UnfairOutcome ;
    rdfs:label "Algorithmic Bias"@en ;
    rdfs:comment "Systematic and repeatable errors in AI systems that create unfair outcomes favoring or disadvantaging certain groups"@en .

aigo:RepresentationBias rdf:type owl:Class ;
    rdfs:subClassOf aigo:BiasAndFairness ;
    rdfs:subClassOf aigo:DataQualityIssue ;
    rdfs:subClassOf aigo:UnbalancedRepresentation ;
    rdfs:label "Representation Bias"@en ;
    rdfs:comment "Bias arising when training data does not adequately represent the diversity of the population the model will serve"@en .

aigo:SamplingBias rdf:type owl:Class ;
    rdfs:subClassOf aigo:BiasAndFairness ;
    rdfs:subClassOf aigo:DataCollectionIssue ;
    rdfs:subClassOf aigo:NonRepresentativeSample ;
    rdfs:label "Sampling Bias"@en ;
    rdfs:comment "Bias introduced when data collection process systematically excludes or over-represents certain populations"@en .

# === Safety and Robustness ===

aigo:SafetyAndRobustness rdf:type owl:Class ;
    rdfs:subClassOf aigo:AIRisk ;
    rdfs:subClassOf aigo:ReliabilityConcern ;
    rdfs:subClassOf aigo:FailureMode ;
    rdfs:label "Safety and Robustness"@en ;
    rdfs:comment "Risk category addressing AI system reliability, predictability, and resilience to unexpected inputs or conditions"@en .

aigo:AdversarialRobustness rdf:type owl:Class ;
    rdfs:subClassOf aigo:SafetyAndRobustness ;
    rdfs:subClassOf aigo:AttackResistance ;
    rdfs:subClassOf aigo:InputPerturbationResilience ;
    rdfs:label "Adversarial Robustness"@en ;
    rdfs:comment "AI system's ability to maintain correct performance when facing intentionally crafted adversarial inputs"@en .

aigo:OutOfDistributionDetection rdf:type owl:Class ;
    rdfs:subClassOf aigo:SafetyAndRobustness ;
    rdfs:subClassOf aigo:AnomalyDetection ;
    rdfs:subClassOf aigo:UncertaintyEstimation ;
    rdfs:label "Out-of-Distribution Detection"@en ;
    rdfs:comment "Capability to identify when input data significantly differs from training distribution to prevent unreliable predictions"@en .

# === Governance Frameworks ===

aigo:GovernanceFramework rdf:type owl:Class ;
    rdfs:subClassOf aigo:AIGovernance ;
    rdfs:subClassOf aigo:PolicyStructure ;
    rdfs:subClassOf aigo:ComplianceMechanism ;
    rdfs:label "Governance Framework"@en ;
    rdfs:comment "Structured approach to managing AI development, deployment, and monitoring through policies, processes, and accountability"@en .

aigo:AIEthics rdf:type owl:Class ;
    rdfs:subClassOf aigo:GovernanceFramework ;
    rdfs:subClassOf aigo:MoralPrinciples ;
    rdfs:subClassOf aigo:ValueAlignment ;
    rdfs:label "AI Ethics"@en ;
    rdfs:comment "Principles and guidelines ensuring AI systems align with human values, rights, and societal norms"@en .

aigo:RegulatoryCompliance rdf:type owl:Class ;
    rdfs:subClassOf aigo:GovernanceFramework ;
    rdfs:subClassOf aigo:LegalRequirement ;
    rdfs:subClassOf aigo:StandardsAdherence ;
    rdfs:label "Regulatory Compliance"@en ;
    rdfs:comment "Adherence to legal regulations and industry standards governing AI system development and deployment"@en .

aigo:EUAIActCompliance rdf:type owl:Class ;
    rdfs:subClassOf aigo:RegulatoryCompliance ;
    rdfs:subClassOf aigo:RiskBasedRegulation ;
    rdfs:subClassOf aigo:TransparencyRequirement ;
    rdfs:label "EU AI Act Compliance"@en ;
    rdfs:comment "Compliance with European Union's AI Act regulation categorizing AI systems by risk level and imposing corresponding requirements"@en .

# === Explainability Approaches ===

aigo:ExplainabilityApproach rdf:type owl:Class ;
    rdfs:subClassOf aigo:AIGovernance ;
    rdfs:subClassOf aigo:TransparencyMechanism ;
    rdfs:subClassOf aigo:InterpretabilityMethod ;
    rdfs:label "Explainability Approach"@en ;
    rdfs:comment "Methodology for making AI system decision-making processes understandable to humans"@en .

aigo:Interpretability rdf:type owl:Class ;
    rdfs:subClassOf aigo:ExplainabilityApproach ;
    rdfs:subClassOf aigo:ModelUnderstanding ;
    rdfs:subClassOf aigo:IntrinsicTransparency ;
    rdfs:label "Interpretability"@en ;
    rdfs:comment "Property of models being inherently understandable in how they make decisions based on their structure"@en .

aigo:Transparency rdf:type owl:Class ;
    rdfs:subClassOf aigo:ExplainabilityApproach ;
    rdfs:subClassOf aigo:Openness ;
    rdfs:subClassOf aigo:Disclosure ;
    rdfs:label "Transparency"@en ;
    rdfs:comment "Openness about AI system design, training data, capabilities, and limitations to enable informed assessment"@en .

aigo:Accountability rdf:type owl:Class ;
    rdfs:subClassOf aigo:ExplainabilityApproach ;
    rdfs:subClassOf aigo:ResponsibilityAssignment ;
    rdfs:subClassOf aigo:AuditTrail ;
    rdfs:label "Accountability"@en ;
    rdfs:comment "Clear assignment of responsibility for AI system decisions and mechanisms to trace and explain outcomes"@en .

# === Risk Assessment and Mitigation ===

aigo:RiskAssessment rdf:type owl:Class ;
    rdfs:subClassOf aigo:AIGovernance ;
    rdfs:subClassOf aigo:ImpactEvaluation ;
    rdfs:subClassOf aigo:HarmPrediction ;
    rdfs:label "Risk Assessment"@en ;
    rdfs:comment "Systematic process of identifying, analyzing, and evaluating potential risks associated with AI systems"@en .

aigo:RiskMitigation rdf:type owl:Class ;
    rdfs:subClassOf aigo:AIGovernance ;
    rdfs:subClassOf aigo:HarmReduction ;
    rdfs:subClassOf aigo:SafeguardImplementation ;
    rdfs:label "Risk Mitigation"@en ;
    rdfs:comment "Strategies and techniques to reduce identified AI risks to acceptable levels through preventive and corrective measures"@en .

aigo:ContinuousMonitoring rdf:type owl:Class ;
    rdfs:subClassOf aigo:RiskMitigation ;
    rdfs:subClassOf aigo:OngoingEvaluation ;
    rdfs:subClassOf aigo:PerformanceTracking ;
    rdfs:label "Continuous Monitoring"@en ;
    rdfs:comment "Ongoing observation and analysis of AI system behavior in production to detect emerging risks and performance degradation"@en .

aigo:IncidentResponse rdf:type owl:Class ;
    rdfs:subClassOf aigo:RiskMitigation ;
    rdfs:subClassOf aigo:FailureHandling ;
    rdfs:subClassOf aigo:RecoveryProtocol ;
    rdfs:label "Incident Response"@en ;
    rdfs:comment "Structured approach to addressing and managing AI system failures, errors, or harmful outcomes when they occur"@en .

# ============================================================================
# CATEGORY 4: TRAINING & DATA (15 classes)
# Dataset types, preprocessing, validation strategies, metrics, quality
# ============================================================================

# === Dataset Types ===

aigo:Dataset rdf:type owl:Class ;
    rdfs:label "Dataset"@en ;
    rdfs:comment "Structured collection of data used for training, validating, or testing machine learning models"@en .

aigo:SupervisedDataset rdf:type owl:Class ;
    rdfs:subClassOf aigo:Dataset ;
    rdfs:subClassOf aigo:LabeledDataRequired ;
    rdfs:subClassOf aigo:InputOutputPairs ;
    rdfs:label "Supervised Dataset"@en ;
    rdfs:comment "Dataset containing labeled examples with explicit input-output mappings for supervised learning"@en .

aigo:UnsupervisedCorpus rdf:type owl:Class ;
    rdfs:subClassOf aigo:Dataset ;
    rdfs:subClassOf aigo:UnlabeledDataProcessing ;
    rdfs:subClassOf aigo:RawDataCollection ;
    rdfs:label "Unsupervised Corpus"@en ;
    rdfs:comment "Large collection of unlabeled data used for unsupervised learning and self-supervised pretraining"@en .

aigo:BenchmarkDataset rdf:type owl:Class ;
    rdfs:subClassOf aigo:Dataset ;
    rdfs:subClassOf aigo:StandardizedEvaluation ;
    rdfs:subClassOf aigo:ModelComparison ;
    rdfs:label "Benchmark Dataset"@en ;
    rdfs:comment "Standardized dataset used to evaluate and compare performance of different machine learning models"@en .

# === Data Preprocessing ===

aigo:DataPreprocessing rdf:type owl:Class ;
    rdfs:subClassOf aigo:DataPipeline ;
    rdfs:subClassOf aigo:DataTransformation ;
    rdfs:label "Data Preprocessing"@en ;
    rdfs:comment "Process of transforming raw data into clean, formatted data suitable for machine learning model training"@en .

aigo:Normalization rdf:type owl:Class ;
    rdfs:subClassOf aigo:DataPreprocessing ;
    rdfs:subClassOf aigo:ScaleAdjustment ;
    rdfs:subClassOf aigo:DistributionStandardization ;
    rdfs:label "Normalization"@en ;
    rdfs:comment "Rescaling numerical features to standard range or distribution to improve model training stability"@en .

aigo:DataAugmentation rdf:type owl:Class ;
    rdfs:subClassOf aigo:DataPreprocessing ;
    rdfs:subClassOf aigo:SyntheticDataGeneration ;
    rdfs:subClassOf aigo:VarietyEnhancement ;
    rdfs:label "Data Augmentation"@en ;
    rdfs:comment "Technique creating modified copies of existing data to increase training set size and diversity"@en .

aigo:FeatureEngineering rdf:type owl:Class ;
    rdfs:subClassOf aigo:DataPreprocessing ;
    rdfs:subClassOf aigo:RepresentationOptimization ;
    rdfs:subClassOf aigo:DomainKnowledgeApplication ;
    rdfs:label "Feature Engineering"@en ;
    rdfs:comment "Process of selecting, transforming, and creating features from raw data to improve model performance"@en .

# === Validation Strategies ===

aigo:ValidationStrategy rdf:type owl:Class ;
    rdfs:subClassOf aigo:ModelEvaluation ;
    rdfs:subClassOf aigo:GeneralizationAssessment ;
    rdfs:label "Validation Strategy"@en ;
    rdfs:comment "Methodology for assessing model performance on unseen data to estimate generalization capability"@en .

aigo:CrossValidation rdf:type owl:Class ;
    rdfs:subClassOf aigo:ValidationStrategy ;
    rdfs:subClassOf aigo:MultiplePartitioning ;
    rdfs:subClassOf aigo:RobustEvaluation ;
    rdfs:label "Cross-Validation"@en ;
    rdfs:comment "Validation technique partitioning data into multiple folds for repeated training and testing to reduce evaluation variance"@en .

aigo:TrainTestSplit rdf:type owl:Class ;
    rdfs:subClassOf aigo:ValidationStrategy ;
    rdfs:subClassOf aigo:DataPartitioning ;
    rdfs:subClassOf aigo:HoldoutMethod ;
    rdfs:label "Train-Test Split"@en ;
    rdfs:comment "Simple validation approach dividing dataset into separate training and testing subsets"@en .

aigo:HoldoutValidation rdf:type owl:Class ;
    rdfs:subClassOf aigo:ValidationStrategy ;
