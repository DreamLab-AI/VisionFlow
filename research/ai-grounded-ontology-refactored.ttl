# === Machine Learning Model Constraints ===

aigo:MachineLearningModelShape
  a sh:NodeShape ;
  sh:targetClass aigo:MachineLearningModel ;
  sh:property [
    sh:path aigo:hasAccuracy ;
    sh:datatype xsd:float ;
    sh:minInclusive 0.0 ;
    sh:maxInclusive 1.0 ;
    sh:message "Accuracy must be a float between 0.0 and 1.0"@en ;
  ] ;
  sh:property [
    sh:path aigo:hasPrecision ;
    sh:datatype xsd:float ;
    sh:minInclusive 0.0 ;
    sh:maxInclusive 1.0 ;
    sh:message "Precision must be a float between 0.0 and 1.0"@en ;
  ] ;
  sh:property [
    sh:path aigo:hasRecall ;
    sh:datatype xsd:float ;
    sh:minInclusive 0.0 ;
    sh:maxInclusive 1.0 ;
    sh:message "Recall must be a float between 0.0 and 1.0"@en ;
  ] ;
  sh:property [
    sh:path aigo:hasF1Score ;
    sh:datatype xsd:float ;
    sh:minInclusive 0.0 ;
    sh:maxInclusive 1.0 ;
    sh:message "F1 Score must be a float between 0.0 and 1.0"@en ;
  ] ;
  sh:property [
    sh:path aigo:numberOfParameters ;
    sh:datatype xsd:long ;
    sh:minInclusive 1 ;
    sh:message "Number of parameters must be a positive long integer"@en ;
  ] ;
  sh:property [
    sh:path aigo:modelSize ;
    sh:datatype xsd:long ;
    sh:minInclusive 0 ;
    sh:message "Model size must be a non-negative long integer (bytes)"@en ;
  ] .

# === Neural Network Constraints ===

aigo:NeuralNetworkShape
  a sh:NodeShape ;
  sh:targetClass aigo:NeuralNetworkArchitecture ;
  sh:property [
    sh:path aigo:numberOfLayers ;
    sh:datatype xsd:positiveInteger ;
    sh:minInclusive 1 ;
    sh:message "Neural network must have at least 1 layer"@en ;
  ] ;
  sh:property [
    sh:path aigo:hasArchitecture ;
    sh:minCount 1 ;
    sh:message "Neural network must specify an architecture"@en ;
  ] .

# === AI Risk Constraints ===

aigo:AIRiskShape
  a sh:NodeShape ;
  sh:targetClass aigo:AIRisk ;
  sh:property [
    sh:path aigo:riskScore ;
    sh:datatype xsd:float ;
    sh:minInclusive 0.0 ;
    sh:maxInclusive 1.0 ;
    sh:message "Risk score must be between 0.0 (minimal) and 1.0 (critical)"@en ;
  ] ;
  sh:property [
    sh:path aigo:probabilityOfHarm ;
    sh:datatype xsd:float ;
    sh:minInclusive 0.0 ;
    sh:maxInclusive 1.0 ;
    sh:message "Probability of harm must be between 0.0 and 1.0"@en ;
  ] ;
  sh:property [
    sh:path aigo:severityOfHarm ;
    sh:datatype xsd:string ;
    sh:in ( "negligible" "low" "moderate" "high" "critical" ) ;
    sh:message "Severity must be: negligible, low, moderate, high, or critical"@en ;
  ] .

# === AI System Constraints ===

aigo:AISystemShape
  a sh:NodeShape ;
  sh:targetClass aigo:AISystem ;
  sh:property [
    sh:path aigo:riskLevel ;
    sh:datatype xsd:string ;
    sh:in ( "unacceptable" "high" "limited" "minimal" ) ;
    sh:message "Risk level must be: unacceptable, high, limited, or minimal (EU AI Act)"@en ;
  ] ;
  sh:property [
    sh:path rdfs:label ;
    sh:minCount 1 ;
    sh:datatype xsd:string ;
    sh:message "AI System must have at least one label"@en ;
  ] .

# === Training Data Constraints ===

aigo:TrainingDataShape
  a sh:NodeShape ;
  sh:targetClass aigo:Dataset ;
  sh:property [
    sh:path aigo:datasetSize ;
    sh:datatype xsd:long ;
    sh:minInclusive 1 ;
    sh:message "Dataset size must be at least 1"@en ;
  ] ;
  sh:property [
    sh:path aigo:dataQualityScore ;
    sh:datatype xsd:float ;
    sh:minInclusive 0.0 ;
    sh:maxInclusive 1.0 ;
    sh:message "Data quality score must be between 0.0 and 1.0"@en ;
  ] ;
  sh:property [
    sh:path aigo:hasLabel ;
    sh:datatype xsd:boolean ;
    sh:message "hasLabel must be a boolean value"@en ;
  ] .

